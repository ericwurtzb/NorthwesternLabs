@model IEnumerable<NorthwesternLabs.Models.AssayInfo>

@{
    ViewBag.Title = "ViewAssays";
}

<h2 align="center">Assays</h2>

<br />
<table class="table" align="center">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CompoundSequenceCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Desc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompletionEstimate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateTimeScheduled)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExtraTestNotes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResultsLink)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CompoundSequenceCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompletionEstimate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateTimeScheduled)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExtraTestNotes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResultsLink)
        </td>
    </tr>
}

</table>
